<launch>

<node pkg="hbv_1780_ros" type="hbv_1780_node" name="hbv_1780_left" output="screen">
    <param name="left_camera_config_file" value="$(find hbv_1780_ros)/../../config/hbv_1780_left/left.yaml" />
    <param name="right_camera_config_file" value="$(find hbv_1780_ros)/../../config/hbv_1780_left/right.yaml" />
    <param name="device" value="/dev/video6" />
    <param name="pub_whole" value="false" />
</node>

